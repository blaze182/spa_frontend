{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "semi": ["error", "never"],
    "max-len": [1, 120, 2, {ignoreComments: true}],
    "react/prop-types": [2],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  }
}
